{
  "scopeName": "source.ku",
  "patterns": [
    {
      "match": "\\b(summon|call the|if|echo|remembers|count from)\\b",
      "name": "keyword.control.ku"
    },
    {
      "match": "\\b(one|two|three|four|five|six|seven|eight|nine|ten)\\b",
      "name": "constant.numeric.ku"
    },
    {
      "match": "\\b(plus|minus|greater)\\b",
      "name": "keyword.operator.ku"
    },
    {
      "match": "//.*$",
      "name": "comment.line.ku"
    }
  ]
} 